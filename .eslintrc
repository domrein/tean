{
  "extends": "eslint:recommended",
  "rules": {
    "array-bracket-spacing": 1,
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": 1,
    "brace-style": [1, "stroustrup"],
    "camelcase": 1,
    "comma-dangle": [1, "always-multiline"],
    "comma-spacing": 1,
    "comma-style": [1, "last"],
    "consistent-return": 2,
    "constructor-super": 2,
    "curly": 1,
    "dot-notation": 1,
    "eqeqeq": 2,
    "init-declarations": 2,
    "key-spacing": 1,
    "no-console": 0,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-native-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-shadow": 0,
    "no-sync": 2,
    "no-trailing-spaces": 1,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "no-use-before-define": 2,
    "no-var": 1,
    "no-with": 2,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-template": 1,
    "quotes": 1,
    "semi": 1,
    "keyword-spacing": 1,
    "space-in-parens": [1, "never"],
    "space-infix-ops": 1,
    "spaced-comment": 1,
    "strict": [2, "global"],
    "yoda": 1,
    "space-unary-ops": 1
  },
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
